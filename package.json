{
  "name": "nft-go",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "android:emulator": "export ANDROID_HOME=$HOME/Library/Android/sdk && export PATH=$PATH:$ANDROID_HOME/emulator:$ANDROID_HOME/platform-tools && emulator -list-avds && echo 'Starting first available emulator...' && emulator -avd $(emulator -list-avds | head -1) &",
    "ios:simulator": "open -a Simulator && xcrun simctl boot 'iPhone 15 Pro' 2>/dev/null || echo 'Simulator already running or device not found'"
  },
